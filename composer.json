{
    "name": "veronalabs/framework",
    "description": "",
    "keywords": [
        "wordpress",
        "pressmodo",
        "sematico",
        "backyard",
        "framework"
    ],
    "homepage": "https://backyard.sematico.com",
    "license": "GPL-3.0-or-later",
    "authors": [
        {
            "name": "Alessandro Tesoro",
            "email": "hello@sematico.com",
            "homepage": "https://sematico.com",
            "role": "Founder & MD"
        }
    ],
    "support": {
        "issues": "https://github.com/backyardwp/framework/issues"
    },
    "autoload": {
        "psr-4": {
            "Backyard\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Backyard\\Tests\\": "tests"
        }
    },
    "require": {
        "php": ">=7.2",
        "caseyamcl/configula": "^3.1",
        "illuminate/database": "^8.61",
        "illuminate/macroable": "^8.7",
        "illuminate/view": "^8.61",
        "laminas/laminas-diactoros": "^2.5",
        "laminas/laminas-escaper": "^2.7",
        "laminas/laminas-form": "^2.15",
        "laminas/laminas-i18n": "^2.11",
        "laminas/laminas-view": "^2.11",
        "league/container": "^3.3",
        "monolog/monolog": "^2.3",
        "symfony/finder": "^5.1",
        "symfony/http-foundation": "^5.1"
    },
    "require-dev": {
        "dealerdirect/phpcodesniffer-composer-installer": "^0.7.0",
        "mikey179/vfsstream": "^1.6",
        "phpunit/phpunit": "^7.0",
        "symfony/var-dumper": "^5.1",
        "wp-coding-standards/wpcs": "dev-master"
    },
    "config": {
        "bin-dir": "bin",
        "sort-packages": true,
        "preferred-install": {
            "*": "dist"
        },
        "platform-check": false,
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    },
    "scripts": {
        "test": "bin/phpunit"
    },
    "prefer-stable": true
}
